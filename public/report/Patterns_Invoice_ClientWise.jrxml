	<queryString language="SQL">
		<![CDATA[SELECT honda.orders.client_creation_id,
	honda.orders.order_id,
	honda.orders.order_date_time,
	honda.orders.client_name,
	honda.orders.client_email_primary,
	honda.orders.client_company,
	honda.orders.client_address1,
	honda.orders.client_state,
	honda.orders.client_contact_1,
	honda.orders.client_note,
	honda.orders.file_name,
	honda.orders.stiches_count,
	honda.orders.file_price,
	honda.orders.unit_price,
	honda.orders.file_type
FROM honda.orders
WHERE 
	 honda.orders.order_date_time >= $P{PFR_DT} 
	 AND honda.orders.order_date_time <= $P{PTO_DT} 
	 AND honda.orders.client_creation_id =  $P{P_client_creation_id} 
	 AND honda.orders.file_price > 0
	  AND honda.orders.file_type in ('Vector', 'Digitizing')
GROUP BY honda.orders.client_creation_id,
	honda.orders.order_id,
	honda.orders.order_date_time,
	honda.orders.client_name,
	honda.orders.client_email_primary,
	honda.orders.client_company,
	honda.orders.client_address1,
