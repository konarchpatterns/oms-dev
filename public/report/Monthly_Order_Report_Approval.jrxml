date_format(approval_time,"%Y-%m-%d %H:%i")      between   date_format($P{PFR_DT}, "%Y-%m-%d %H:%i") and 
 date_format($P{PTO_DT} , "%Y-%m-%d %H:%i") order by  company_id, client_company, client_name,
file_type]]>
