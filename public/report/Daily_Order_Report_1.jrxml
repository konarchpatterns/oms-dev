FROM  orders
where  $P!{param2}  and status  in ('Completed', 'Approved', 'Alloted', 'QC Pending', 'QC OK', 'Revision' , 'Changes', 'Ch-Alloted',
	'Ch-QC Pending', 'Ch-QC OK', 'Ch-Completed', 'Rev-Alloted', 'Rev-QC Pending', 'Rev-QC OK', 'Rev-Completed'
)  and  date_format(bill_dt,"%Y-%m-%d") =  $P{PFR_DT}  order by  company_id, client_company, client_name,
file_type]]>
