	<queryString language="SQL">
		<![CDATA[SELECT orders.company_id,
    orders.client_creation_id,
	orders.order_id,
	orders.order_date_time,
	orders.client_name,
	orders.client_email_primary,
	orders.client_company,
	orders.client_address1,
	orders.client_state,
	orders.client_contact_1,
	orders.client_note,
	SUBSTR(trim(orders.file_name),1,20)  as file_name,
	orders.stiches_count,
	orders.file_price,
	orders.unit_price,
	orders.file_type,
	orders.document_type,
	orders.order_us_date,
   IFNULL(trim(orders.note), '-')  as ordernote
FROM orders
WHERE 
	date_format(orders.bill_dt,"%Y-%m-%d") >= $P{PFR_DT} 
	 AND date_format(orders.bill_dt,'%Y-%m-%d') <= $P{PTO_DT} 
	 AND orders.company_id =  $P{P_company_id} 
	 AND orders.file_price > 0
	 $P!{Param2}
		Order BY orders.company_id, 
  orders.order_us_date]]>
	</queryString>
